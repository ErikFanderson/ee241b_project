#----------------------------------------------------------
# Target "build" 
#----------------------------------------------------------
build: 
	toolbox-cli -c config/jobs.yml build	
.PHONY: build
#----------------------------------------------------------

#----------------------------------------------------------
# Target "fpga_simple" 
#----------------------------------------------------------
fpga_simple: 
	toolbox-cli -c config/jobs.yml fpga_simple	
.PHONY: fpga_simple
#----------------------------------------------------------

#----------------------------------------------------------
# Target "list" - lists all found jobs
#----------------------------------------------------------
list:
	@echo "#--------------------------------------------------------------------"
	@echo "# Toolbox generated make targets"
	@echo "#--------------------------------------------------------------------"
	@echo "build          Builds a makefile for running toolbox jobs"
	@echo "fpga_simple    Builds a very simple FPGA for evaluation purposes"
	@echo "#--------------------------------------------------------------------"
.PHONY: list 
#----------------------------------------------------------